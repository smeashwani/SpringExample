<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans  
               http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="address" class="com.training.springcore.model.Address">
		<property name="city" value="ghaziabad"></property>
		<property name="state" value="UP"></property>
	</bean>
	<bean id="java" class="java.lang.String">
		<constructor-arg name="original" value="welcome to java"></constructor-arg>
	</bean>
	<bean id="empBean" class="com.training.springcore.model.Employee1">
		<property name="name">
			<ref bean="java" />
		</property>
		<property name="age" value="18"></property>
		<property name="salary" value="18000"></property>
		<property name="address">
			<ref bean="address"/>
		</property>
	
		<property name="qualification">
			<list>
				<value>Java is a programming language</value>
				<value>Java is a platform</value>
				<ref bean="java" />
			</list>
		</property>
		<property name="answers">
			<map>
				<entry key="address" value-ref="address"></entry>
				<entry key="key1" value="DUCAT"></entry>
				<entry key="answer2" value="user2"></entry>
			</map>
		</property>
	</bean>

	

</beans>
